{
  "problemMatcher": [
    {
      "owner": "clint",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+):(\\d+):\\s*([a-z][a-z0-9-]*):\\s*(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 5,
          "code": 4
        }
      ]
    }
  ]
}
