{
  "compilerOptions": {
    "target": "es2020",
  "module": "commonjs",
  "moduleResolution": "node10",
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@opentelemetry/sdk-node": ["../node_modules/@opentelemetry/sdk-node"]
    }
  },
  "include": ["src/**/*"]
}
