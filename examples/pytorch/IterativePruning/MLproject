name: alexnet-cifar10-classification

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      max_epochs: {type: int, default: 3}
      mlflow_run_name: {type: str, default: 'BaseModel'}

    command: |
          python alexnet.py \
            --max_epochs {max_epochs} \
            --mlflow_run_name {mlflow_run_name}