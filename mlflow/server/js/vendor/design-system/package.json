{
  "name": "@databricks/design-system",
  "version": "1.0.0-00454544",
  "description": "DuBois Design System",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/databricks"
  },
  "databricksMeta": {
    "transpileSources": true
  },
  "main": "dist",
  "types": "dist/index.d.ts",
  "// exports": [
    "Used by Jest and point to dist. Webpack and Storybook use databricksExports and point to src."
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./development": {
      "types": "./dist/development/index.d.ts",
      "import": "./dist/development.js",
      "require": "./dist/development.js"
    },
    "./patterns": {
      "types": "./dist/~patterns/index.d.ts",
      "import": "./dist/patterns.js",
      "require": "./dist/patterns.js"
    },
    "./test-utils/*": {
      "types": "./dist/test-utils/*/index.d.ts",
      "import": "./dist/test-utils/*",
      "require": "./dist/test-utils/*"
    },
    "./index.css": "./dist/index.css",
    "./index-dark.css": "./dist/index-dark.css",
    "./dist/index.css": "./dist/index.css",
    "./dist/index-dark.css": "./dist/index-dark.css",
    "./dist/dubois-colors.less": "./dist/dubois-colors.less"
  },
  "databricksExports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./development": {
      "import": "./src/development/index.ts",
      "require": "./src/development/index.ts"
    },
    "./patterns": {
      "import": "./src/~patterns/index.ts",
      "require": "./src/~patterns/index.ts"
    },
    "./test-utils/*": {
      "import": "./src/test-utils/*",
      "require": "./src/test-utils/*"
    },
    "./index.css": "./dist/index.css",
    "./index-dark.css": "./dist/index-dark.css",
    "./dist/index.css": "./dist/index.css",
    "./dist/index-dark.css": "./dist/index-dark.css",
    "./dist/dubois-colors.less": "./dist/dubois-colors.less"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "test-utils"
  ],
  "scripts": {
    "prepublish": "./scripts/prepublish",
    "postpublish": "./scripts/postpublish",
    "generate": "npm-run-all generate:icons",
    "generate:icons": "rm -rf src/design-system/Icon/__generated/icons && svgr -d src/design-system/Icon/__generated/icons --typescript --icon --template src/design-system/Icon/generator/iconTemplate.js src/assets/icons && yarn lint:fix && yarn workspace @databricks/du-bois-storybook generate && yarn workspace @databricks/du-bois-storybook prettier:fix",
    "clean": "rm -rf dist '*.tsbuildinfo'",
    "build": "DATABRICKS_DESIGN_SYSTEM_BUILD_SOURCES=false yarn databricks-pkgr build --no-emit-types && yarn build:less-colors",
    "build:dist": "DATABRICKS_DESIGN_SYSTEM_BUILD_SOURCES=true yarn databricks-pkgr build --no-emit-types && yarn build:less-colors",
    "emit-types": "databricks-pkgr emit-types",
    "build:ci": "DATABRICKS_DESIGN_SYSTEM_BUILD_SOURCES=true databricks-pkgr build && yarn build:less-colors",
    "build:deps": "yarn workspaces foreach -piR --topological-dev --from @databricks/design-system --exclude @databricks/design-system run build",
    "build:less-colors": "swc-node ./scripts/less_color_vars_generation.ts",
    "watch": "echo \"\n*******************\nyarn watch is no longer needed for $npm_package_name. Watching is done by the dev-proxy server automatically.\n*******************\n\"; exit 1",
    "gocx-codegen": "../js/scripts/bin/gocx-codegen",
    "safex-codegen": "yarn run gocx-codegen",
    "indexes-regenerate": "swc-node ./scripts/index_regeneration/regenerate.ts",
    "indexes-check": "swc-node ./scripts/index_regeneration/check.ts",
    "theme-regenerate": "swc-node ./scripts/theme_regeneration/regenerate_all.ts && databricks-prettier ./src/theme/_generated --write",
    "theme-check": "swc-node ./scripts/theme_regeneration/check.ts",
    "prettier:fix": "databricks-prettier . --write",
    "prettier:check": "databricks-prettier . --check",
    "lint": "databricks-eslint",
    "lint:fix": "yarn lint --fix",
    "test": "databricks-jest",
    "test:watch": "yarn test --watch",
    "type-check": "tsc --noEmit --project ./tsconfig.json",
    "type-check:watch": "yarn type-check --watch",
    "type-check:cypress": "tsc --noEmit --project ./cypress/tsconfig.json",
    "check-all": "yarn type-check && yarn prettier:check && yarn lint",
    "fix-all": "yarn generate && yarn prettier:fix && yarn indexes-regenerate && yarn lint:fix && yarn type-check",
    "prep-deps": ": # magic task to allow yarn ensure-deps plugin prepare dependencies of this package"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
